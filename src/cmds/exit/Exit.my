package embox.cmd

@AutoCmd
@Cmd(name="exit",
	help="Exit from shell",
	man="")
module exit {
	source "exit.c"
	depends embox.compat.posix.proc.exit
	depends embox.compat.libc.stdlib.core
}

@AutoCmd
@Cmd(name="logout",
	help="Logout current user",
	man="")
module logout {
	source "logout.c"
	depends embox.compat.posix.proc.exit
}
